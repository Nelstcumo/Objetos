int[][] malla_actual;
int[][] malla_next;

int desface = 300;
int tamano_celda = 20;
boolean pause = true;
int vecinos;

PImage img;

void setup(){
  size(1000,700);
  frameRate(10);
  img = loadImage("panel.jpg");
  malla_actual = new int[(width-desface)/tamano_celda][height/tamano_celda];
  malla_next = new int[(width-desface)/tamano_celda][height/tamano_celda];
  crear_malla();  //crea el arreglo bidimensional.
}

void draw(){ 
  image(img, 700, 0);
  init_malla();  //pinta el arreglo.
  if (pause == false){
    next_gen();  //evalúa la cantidad de vecinos de cada célula.
    actualizar();  //actualiza el arreglo.
  }  
}


void crear_malla(){        
  for (int i = 0; i < malla_actual.length; i++){
    for (int j = 0; j < malla_actual[0].length; j++){   
      malla_actual[i][j] = 0;
      malla_next[i][j] = 0;
      rect(i*tamano_celda,j*tamano_celda,tamano_celda,tamano_celda);                                                    
    }
  }
}

void init_malla(){
  for (int i = 0; i < malla_actual.length; i++){
    for (int j = 0; j < malla_actual[0].length; j++){ 
      if (malla_actual[i][j]== 1){
        fill(0);
        rect(i*tamano_celda,j*tamano_celda,tamano_celda,tamano_celda);
      }else {
        fill(255);
        rect(i*tamano_celda,j*tamano_celda,tamano_celda,tamano_celda);
      }    
    }
  }
}

void next_gen(){
  for (int i = 1; i < malla_actual.length-1; i++){
    for (int j = 1; j < malla_actual[0].length-1; j++){
      vecinos = 0;
      if (malla_actual[i][j-1]==1){
        vecinos += 1;
      } 
      if (malla_actual[i+1][j-1]==1){
        vecinos += 1;
      }
      if (malla_actual[i+1][j]==1){
        vecinos += 1;
      }
      if (malla_actual[i+1][j+1]==1){
        vecinos += 1;
      }
      if (malla_actual[i][j+1]==1){
        vecinos += 1;
      }
      if (malla_actual[i-1][j+1]==1){
        vecinos += 1;
      }
      if (malla_actual[i-1][j]==1){
        vecinos += 1;
      }
      if (malla_actual[i-1][j-1]==1){
        vecinos += 1;
      }
      if (malla_actual[i][j] == 0 && vecinos == 3){
        malla_next[i][j] = 1;
      } else if(malla_actual[i][j] == 1 && (vecinos == 3 || vecinos == 2)){
        malla_next[i][j] = 1;
      } else {
        malla_next[i][j] = 0;
      }
    }
  }
}

void actualizar(){
  for (int i = 1; i < malla_actual.length-1; i++){
    for (int j = 1; j < malla_actual[0].length-1; j++){
      malla_actual[i][j] = malla_next[i][j];
    }
  }
}

void mouseDragged(){  //interaccion.
  if (mouseX < width - desface){    
    malla_actual[mouseX/tamano_celda][mouseY/tamano_celda] = 1;
  }
}

void mouseClicked(){  //interaccion.
  if(mouseButton == LEFT && mouseX <= width - desface){
    malla_actual[mouseX/tamano_celda][mouseY/tamano_celda] = 1;
  }
  else if (mouseButton == RIGHT){
    crear_malla();
  }
}


void keyPressed(){              //pausa, opciones del juego.
  if (key == ENTER){
    if (pause == true){
      pause = false;
    } else{
      pause = true;
    }
  } 
  
  if ((width-desface)/tamano_celda == 35 && height/tamano_celda == 35){
    switch(key){
      case '1':
        for (int i = 1; i < malla_actual.length-1; i++){
          for (int j = 1; j < malla_actual[0].length-1; j++){
            malla_actual[i][j] = R_pentomino[i][j];
          }
        }
      break;
      
      case '2':
        for (int i = 1; i < malla_actual.length-1; i++){
          for (int j = 1; j < malla_actual[0].length-1; j++){
            malla_actual[i][j] = carita_feliz[i][j];
          }
        }        
      break;
       
      case '3':
        for (int i = 1; i < malla_actual.length-1; i++){
         for (int j = 1; j < malla_actual[0].length-1; j++){
           malla_actual[i][j] = pseudo_gosper[i][j];
         }
       }       
      break;
       
      case '4':
        for (int i = 1; i < malla_actual.length-1; i++){
          for (int j = 1; j < malla_actual[0].length-1; j++){
            malla_actual[i][j] = int(random(4));
          }
        }
      }
   }
}


